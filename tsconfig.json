{
    "compilerOptions": {
        "target": "ES6",
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowImportingTsExtensions": true,
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "types": ["cypress", "jest", "node"]
    },
    "include": ["src", "cypress"],
    "exclude": ["node_modules", "build", "dist"]
}